/* uni.scss */
.login-cpnt {
  min-height: 100vh;
}
.logo {
  height: 532rpx;
  background: -webkit-linear-gradient(bottom, #1d69f0, #1bb3f9);
  background: linear-gradient(0deg, #1d69f0, #1bb3f9);
}
.logo .logoCard {
  width: 156rpx;
  height: 156rpx;
  border-radius: 42rpx;
}
.logo .logoCard .theLogo {
  width: 114rpx;
  border-radius: unset;
}
.form {
  width: 692rpx;
  margin: 0 auto;
}
.form .formArea {
  box-sizing: border-box;
  width: 100%;
  margin: 0 auto;
  border-radius: 10px;
}
.top108rpx {
  top: -108rpx;
}
.extra {
  width: 100%;
}
.lh1em {
  line-height: 1em;
}

